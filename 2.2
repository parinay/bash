#!/bin/bash

CURRENT=$(date -u)
WHO=$(who)
W=$(w)
UPTIME=$(uptime)
FILE="$0.out"
echo "Filename is - $FILE"
echo "Current date and time is $CURRENT"  | tee > $FILE

echo "All loggedin users are - "  | tee >> $FILE
echo "${WHO}"  | tee >> $FILE 
echo "All logged in users and their processes belonging to them - " \
	| tee >> $FILE
echo "$W"  | tee >> $FILE
echo "The uptime - $UPTIME"  | tee >> $FILE

